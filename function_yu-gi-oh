function myFunction() {
  for ( var i = 1; i <= 100; i++ )
{
  if ( i%3 === 0 && i%5 === 0 )
  {
    console.log( i + "yu " );
  }
  else if ( i%3 === 0 ) 
  {
    console.log(i+ " yu-gi" );
  }
  else if ( i%5 === 0 ) 
  {
    console.log(i+ " yu-gi-oh" );
  }
  else
  {
    console.log(i);
  }
}
}
